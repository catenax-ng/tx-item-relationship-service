        <gav regex="true">org\.yaml:snakeyaml.*</gav>
        <vulnerabilityName>CVE-2022-38752</vulnerabilityName>
    </suppress>
    <suppress>
        <notes><![CDATA[
        False positive, version in use (> 1.31) is not affected anymore.
        ]]></notes>
        <gav regex="true">org\.yaml:snakeyaml.*</gav>
        <vulnerabilityName>CVE-2022-1471</vulnerabilityName>
    </suppress>
    <suppress>
